{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNext",
    "jsx": "react",
    "declaration": false,
    "rootDir": "src",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
